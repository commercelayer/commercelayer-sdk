{
  "name": "@commercelayer/sdk",
  "version": "7.0.0-rc.11",
  "main": "lib/index.cjs",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/index.cjs"
      }
    },
    "./bundle": {
      "import": {
        "types": "./lib/bundle.d.ts",
        "default": "./lib/bundle.js"
      },
      "require": {
        "types": "./lib/bundle.d.cts",
        "default": "./lib/bundle.cjs"
      }
    }
  },
  "scripts": {
    "clean": "rm -rf ./.nyc_output ./node_modules/.cache ./coverage",
    "build": "tsup",
    "generate": "tsx gen/generator.ts",
    "generate-local": "tsx gen/generator.ts -- --local",
    "lint": "eslint ./src --ext .ts",
    "lintspec": "eslint ./specs/resources/ --ext .spec.ts",
    "release": "pnpm upgrade && pnpm generate && pnpm build && pnpm test",
    "semantic-release": "semantic-release",
    "test": "vitest",
    "test-local": "tsx test/spot.ts",
    "test-tree": "tsx test/tree.ts",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "javascript",
    "ecommerce",
    "jamstack",
    "commercelayer"
  ],
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "license": "MIT",
  "description": "Commerce Layer Javascript SDK",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@commercelayer/eslint-config-ts": "^2.2.0",
    "@commercelayer/js-auth": "^6.7.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.15.32",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.2.7",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.1",
    "json-typescript": "^1.1.2",
    "jsonapi-typescript": "^0.1.3",
    "semantic-release": "^24.2.5",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "repository": "github:commercelayer/commercelayer-sdk",
  "bugs": "https://github.com/commercelayer/commercelayer-sdk/issues",
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "ansi-regex": "5.0.1"
  }
}
