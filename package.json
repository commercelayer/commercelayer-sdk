{
  "name": "@commercelayer/sdk",
  "version": "6.48.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "scripts": {
    "clean": "rm -rf ./.nyc_output ./node_modules/.cache ./coverage",
    "build": "tsup",
    "generate": "tsx gen/generator.ts",
    "generate-local": "tsx gen/generator.ts -- --local",
    "lint": "eslint ./src --ext .ts",
    "lintspec": "eslint ./specs/resources/ --ext .spec.ts",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test-local": "tsx test/spot.ts",
    "coverage": "jest --coverage",
    "release": "pnpm upgrade && pnpm generate && pnpm build && pnpm lint && pnpm test",
    "publish-release": "pnpm publish --tag sdk6"
  },
  "keywords": [
    "javascript",
    "ecommerce",
    "jamstack",
    "commercelayer"
  ],
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "license": "MIT",
  "description": "Commerce Layer Javascript SDK",
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@commercelayer/eslint-config-ts": "1.4.5",
    "@commercelayer/js-auth": "^6.7.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.7.0",
    "dotenv": "^16.6.1",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "json-typescript": "^1.1.2",
    "jsonapi-typescript": "^0.1.3",
    "semantic-release": "^24.2.9",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "repository": "github:commercelayer/commercelayer-sdk",
  "bugs": "https://github.com/commercelayer/commercelayer-sdk/issues",
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "ansi-regex": "5.0.1"
  }
}
