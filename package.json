{
  "name": "@commercelayer/sdk",
  "version": "4.19.0",
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "module": "lib/esm/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.json tsconfig.esm.json --verbose",
    "clean": "rm -rf ./.nyc_output ./node_modules/.cache ./coverage",
    "coverage": "jest --coverage",
    "generate": "ts-node gen/generator.ts",
    "lint": "eslint ./src --ext .ts",
    "lintspec": "eslint ./specs/resources/ --ext .spec.ts",
    "postbuild": "minimize-js -d lib",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "start": "ts-node src/index.ts",
    "test": "jest",
    "test-local": "ts-node test/spot.ts"
  },
  "keywords": [
    "javascript",
    "ecommerce",
    "jamstack",
    "commercelayer"
  ],
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "license": "MIT",
  "description": "Commerce Layer Javascript SDK",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=16 || ^14.17"
  },
  "dependencies": {
    "axios": "^1.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/eslint-config-ts": "^0.1.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "husky": "^8.0.1",
    "inflector-js": "^1.0.1",
    "jest": "^29.2.2",
    "json-typescript": "^1.1.2",
    "jsonapi-typescript": "^0.1.3",
    "lodash": "^4.17.21",
    "minimize-js": "^1.3.0",
    "pretty-quick": "^3.1.3",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "repository": "github:commercelayer/commercelayer-sdk",
  "bugs": "https://github.com/commercelayer/commercelayer-sdk/issues",
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "ansi-regex": "5.0.1"
  }
}
